{
    "presets": ["react","es2015","stage-2"],
    "plugins": [
        "transform-decorators-legacy",
        "transform-flow-strip-types",
        "transform-class-properties"
    ],
    "env": {
        "development": {
            "plugins": [
                ["react-transform", {
                    "transforms": [{
                        "transform": "react-transform-hmr",
                        "imports": ["react"],
                        "locals": ["module"]
                    }]
                }]
            ]
        }
    }
}
